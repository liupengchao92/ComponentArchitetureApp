<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lpc.module_main">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComponentArchitecture">
        <activity
            android:name="com.example.lpc.module_main.ui.activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.lpc.module_main.ui.activity.ui.login.LoginActivity"
            android:theme="@style/LoginTheme" />
        <activity
            android:name="com.example.lpc.module_main.ui.activity.ui.register.RegisterActivity"
            android:theme="@style/LoginTheme" />
        <activity
            android:name=".ui.activity.ui.web.CommonWebActivity"
            android:exported="false" />
    </application>

</manifest>